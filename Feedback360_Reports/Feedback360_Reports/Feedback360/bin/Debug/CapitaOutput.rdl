  <DataSets>
    <DataSet Name="dtQuestionaire">
      <Query>
        <DataSourceName>FeedbackDS</DataSourceName>
        <CommandText>Select A.AccountID,P.ProjectID,pm.ProgrammeID,qn.QuestionnaireID,AQ.AssignmentID,t.UserID,ad.AsgnDetailID,qs.QuestionID,a.Code,a.Description AccountDesc,P.Description ProjectDesc,p.StartDate,p.EndDate,pm.ClientName,pm.ColleagueNo,pm.Instructions,pm.ProgrammeDescription,pm.ProgrammeName,qn.QSTNName,qn.QSTNDescription,qn.QSTNEpilogue,qn.QSTNPrologue,qn.QSTNCode,aq.Description AsQnDesc,t.FirstName,t.LastName,t.EmailID,AD.CandidateName,ad.CandidateEmail,ad.RelationShip,ad.SubmitFlag,REPLACE( qs.Description,'[TARGETNAME]',t.FirstName + ' '+ ISNull(t.LastName,'')) QDesc,qs.DescriptionSelf,qs.Hint,qs.Title,qa.Answer from Account a 
INNER JOIN Project P on A.AccountID = P.AccountID
INNER JOIN Programme pm on pm.ProjectID = P.ProjectID
INNER JOIN Questionnaire qn on qn.AccountID = P.AccountID
INNER JOIN AssignQuestionnaire AQ on AQ.QuestionnaireID = qn.QuestionnaireID and aq.AccountID = a.AccountID and aq.ProjecctID = p.ProjectID and aq.ProgrammeID = pm.ProgrammeID
INNER JOIN [User] T on t.UserID=aq.TargetPersonID
INNER JOIN AssignmentDetails AD on AD.AssignmentID = AQ.AssignmentID
INNER JOIN Question qs on qs.QuestionnaireID = qn.QuestionnaireID and qs.AccountID=a.AccountID
INNER JOIN QuestionAnswer qa on qa.QuestionID=qs.QuestionID  and qa.AssignDetId = Ad.AsgnDetailID
WHERE a.AccountID=68 and p.ProjectID=244
and pm.ProgrammeID=537 
Order by qn.QSTNDescription,T.FirstName,Ad.CandidateName, qs.Sequence</CommandText>
      </Query>
