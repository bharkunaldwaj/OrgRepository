  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="RelationShip">
          <DataField>RelationShip</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategoryName">
          <DataField>CategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sum">
          <DataField>Sum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="No_Of_Candidate">
          <DataField>No Of Candidate</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Average">
          <DataField>Average</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        ad.RelationShip, c.CategoryName, SUM(CAST(qa.Answer AS decimal(12, 2))) AS Sum, COUNT(*) AS [No Of Candidate], 
                         CAST(SUM(CAST(qa.Answer AS decimal(12, 2))) / COUNT(*) AS decimal(12, 2)) AS Average
FROM            Account AS a LEFT OUTER JOIN
                         Category AS c ON c.AccountID = a.AccountID LEFT OUTER JOIN
                         Question AS q ON q.CateogryID = c.CategoryID LEFT OUTER JOIN
                         QuestionAnswer AS qa ON qa.QuestionID = q.QuestionID LEFT OUTER JOIN
                         AssignmentDetails AS ad ON ad.AsgnDetailID = qa.AssignDetId
WHERE        (a.AccountID = 17) AND (q.QuestionTypeID = 2) AND (qa.Answer &lt;&gt; ' ') AND (c.CategoryID = 148)
GROUP BY ad.RelationShip, c.CategoryName</CommandText>
      </Query>
    </DataSet>
  </DataSets>
